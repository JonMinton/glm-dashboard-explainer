{
  "name": "Heart Disease",
  "source": "UCI Machine Learning Repository",
  "url": "https://archive.ics.uci.edu/dataset/45/heart+disease",
  "description": "Cardiovascular health indicators from patients at Cleveland Clinic",
  "variables": [
    {
      "name": "age",
      "label": "Age",
      "type": "continuous",
      "description": "Age in years",
      "unit": "years"
    },
    {
      "name": "sex",
      "label": "Sex",
      "type": "binary",
      "description": "Biological sex",
      "levels": ["Female", "Male"]
    },
    {
      "name": "cp",
      "label": "Chest Pain Type",
      "type": "categorical",
      "description": "Type of chest pain experienced",
      "levels": ["Typical angina", "Atypical angina", "Non-anginal pain", "Asymptomatic"]
    },
    {
      "name": "trestbps",
      "label": "Resting BP",
      "type": "continuous",
      "description": "Resting blood pressure on hospital admission",
      "unit": "mm Hg"
    },
    {
      "name": "chol",
      "label": "Cholesterol",
      "type": "continuous",
      "description": "Serum cholesterol",
      "unit": "mg/dl"
    },
    {
      "name": "fbs",
      "label": "Fasting Blood Sugar",
      "type": "binary",
      "description": "Fasting blood sugar > 120 mg/dl",
      "levels": ["No", "Yes"]
    },
    {
      "name": "restecg",
      "label": "Resting ECG",
      "type": "categorical",
      "description": "Resting electrocardiographic results",
      "levels": ["Normal", "ST-T abnormality", "LV hypertrophy"]
    },
    {
      "name": "thalach",
      "label": "Max Heart Rate",
      "type": "continuous",
      "description": "Maximum heart rate achieved during exercise",
      "unit": "bpm"
    },
    {
      "name": "exang",
      "label": "Exercise Angina",
      "type": "binary",
      "description": "Exercise-induced angina",
      "levels": ["No", "Yes"]
    },
    {
      "name": "oldpeak",
      "label": "ST Depression",
      "type": "continuous",
      "description": "ST depression induced by exercise relative to rest",
      "unit": "mm"
    },
    {
      "name": "slope",
      "label": "ST Slope",
      "type": "categorical",
      "description": "Slope of peak exercise ST segment",
      "levels": ["Upsloping", "Flat", "Downsloping"]
    },
    {
      "name": "ca",
      "label": "Vessels Coloured",
      "type": "count",
      "description": "Number of major vessels coloured by fluoroscopy",
      "range": [0, 3]
    },
    {
      "name": "thal",
      "label": "Thalassemia",
      "type": "categorical",
      "description": "Thalassemia status",
      "levels": ["Normal", "Fixed defect", "Reversible defect"]
    },
    {
      "name": "num",
      "label": "Heart Disease",
      "type": "binary",
      "description": "Diagnosis of heart disease (>50% diameter narrowing)",
      "levels": ["No", "Yes"]
    }
  ],
